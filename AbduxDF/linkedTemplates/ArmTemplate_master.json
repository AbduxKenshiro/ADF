{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AbduxDF"
		},
		"AmazonS3bucket_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'AmazonS3bucket'"
		},
		"AzureBlobStorage01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage01'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AmazonS3bucket_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAUZCH4LSYLQRJXL5Y"
		},
		"storageEventTrigger_properties_22_StorageEventTrigger_parameters_fName": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"storageEventTrigger_properties_22_StorageEventTrigger_parameters_cName": {
			"type": "string",
			"defaultValue": "@triggerBody().folderPath"
		},
		"storageEventTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/6fe876fd-ebbf-4c35-93ed-ed4d11723f70/resourceGroups/AbduxGroup/providers/Microsoft.Storage/storageAccounts/abduxstorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AbduxDF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AmazonS3bucket_secretAccessKey": {
						"value": "[parameters('AmazonS3bucket_secretAccessKey')]"
					},
					"AzureBlobStorage01_connectionString": {
						"value": "[parameters('AzureBlobStorage01_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AmazonS3bucket_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS3bucket_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "AbduxDF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"storageEventTrigger_properties_22_StorageEventTrigger_parameters_fName": {
						"value": "[parameters('storageEventTrigger_properties_22_StorageEventTrigger_parameters_fName')]"
					},
					"storageEventTrigger_properties_22_StorageEventTrigger_parameters_cName": {
						"value": "[parameters('storageEventTrigger_properties_22_StorageEventTrigger_parameters_cName')]"
					},
					"storageEventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('storageEventTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AbduxDF_ArmTemplate_0')]"
			]
		}
	]
}